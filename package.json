{
  "name": "gup-from-scratch",
  "version": "0.0.1",
  "scripts": {
    "browserify": "browserify -t hbsfy js/main.js -o public/bundle.js && browserify  -t hbsfy js/second.js -o public/second.js",
    "sass": "sass sass/main.scss > public/main.css",
    "build-dev": "npm run browserify & npm run sass",
    "prestart-dev": "npm run build-dev",
    "start-dev": "node app.js",
    "clean-artifact": "node build/clean_artifact.js",
    "copy-artifact-files": "node build/copy_files.js",
    "uglify-js-files": "node build/uglify_js_files.js",
    "precreate-artifact": "npm run clean-artifact & npm run build-dev",
    "create-artifact": "npm run copy-artifact-files && npm run uglify-js-files"
  },
  "devDependencies": {
    "browser-sync": "^2.7.4",
    "browserify": "~3.46.0",
    "connect-livereload": "~0.4.0",
    "gulp": "^3.8.6",
    "gulp-autoprefixer": "0.0.7",
    "gulp-browserify": "~0.5.0",
    "gulp-clean": "~0.2.4",
    "gulp-concat": "~2.2.0",
    "gulp-define-module": "^0.1.1",
    "gulp-embedlr": "~0.5.2",
    "gulp-handlebars": "^2.2.0",
    "gulp-jshint": "~1.5.5",
    "gulp-livereload": "~1.3.1",
    "gulp-rev": "^3.0.1",
    "gulp-rev-replace": "^0.4.0",
    "gulp-rimraf": "^0.1.1",
    "gulp-sass": "~0.7.1",
    "gulp-util": "~2.2.14",
    "hbsfy": "^2.2.1",
    "lodash": "^3.9.1",
    "merge-stream": "^0.1.7",
    "run-sequence": "^1.0.2",
    "sass": "^0.5.0",
    "tiny-lr": "0.0.7",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.2.1"
  },
  "dependencies": {
    "body-parser": "^1.9.0",
    "cookie-parser": "^1.3.3",
    "express": "~4.1.1",
    "express-handlebars": "1.0.1",
    "handlebars": "^4.0.5",
    "morgan": "^1.6.1",
    "shelljs": "^0.6.0",
    "static-favicon": "^2.0.0-alpha",
    "uglify-js": "^2.6.1"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
