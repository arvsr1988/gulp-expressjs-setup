{
  "name": "gup-from-scratch",
  "version": "0.0.1",
  "scripts": {
    "compile-scripts": "webpack",
    "sass": "sass sass/main.scss > public/main.css",
    "build-dev": "npm run compile-scripts & npm run sass",
    "prestart-dev": "npm run build-dev",
    "start-dev": "node app.js",
    "watch" : "webpack --watch",
    "clean-artifact": "node build/clean_artifact.js",
    "copy-artifact-files": "node build/copy_files.js",
    "precreate-artifact": "npm run clean-artifact & webpack --config=webpack.prod.config.js & npm run sass",
    "create-artifact": "npm run copy-artifact-files && gulp revreplace"
  },
  "devDependencies": {
    "browser-sync": "^2.7.4",
    "connect-livereload": "~0.4.0",
    "gulp": "^3.8.6",
    "gulp-rev": "^3.0.1",
    "gulp-rev-replace": "^0.4.0",
    "gulp-rimraf": "^0.1.1",
    "handlebars-template-loader": "^0.7.0",
    "hbsfy": "^2.2.1",
    "lodash": "^3.9.1",
    "merge-stream": "^0.1.7",
    "run-sequence": "^1.0.2",
    "sass": "^0.5.0",
    "tiny-lr": "0.0.7",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.2.1",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "body-parser": "^1.9.0",
    "cookie-parser": "^1.3.3",
    "express": "~4.1.1",
    "express-handlebars": "1.0.1",
    "handlebars": "^4.0.5",
    "morgan": "^1.6.1",
    "shelljs": "^0.6.0",
    "static-favicon": "^2.0.0-alpha"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
